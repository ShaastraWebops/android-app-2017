<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="shaastra.com.android_app_2017.SummitPartners">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-30dp"
                android:layout_marginBottom="20dp">

                <ImageView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/summit_banner"
                    android:id="@+id/summit_banner"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Partners"
                    android:layout_below="@+id/summit_banner"
                    android:layout_marginTop="-55dp"
                    android:id="@+id/summ_partners_title"
                    android:textSize="20dp"
                    android:textColor="#000"
                    android:textStyle="bold"
                    android:layout_centerHorizontal="true"/>
                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:text="@string/summit_partners_para1"
                    android:layout_below="@id/summ_partners_title"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_marginTop="15dp"
                    android:textColor="#000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/summit_partners_para1"
                    android:autoLink="web"
                    android:textColorLink="#FF164165"/>



                <ImageView
                    android:id="@+id/pic1"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/accessible_campaign"
                    android:layout_alignTop="@+id/summit_partners_para1"
                    android:layout_toLeftOf="@+id/summit_partners_para1"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="50dp"/>


                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="Accessible India Campaign"
                    android:layout_below="@+id/pic1"
                    android:layout_marginTop="3dp"
                    android:textColor="#333"
                    android:textSize="16sp"/>
                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:text="@string/summit_partners_para2"
                    android:layout_below="@id/summit_partners_para1"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_marginTop="15dp"
                    android:textColor="#000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/summit_partners_para2"
                    android:autoLink="web"
                    android:textColorLink="#FF164165"/>



                <ImageView
                    android:id="@+id/pic2"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/deque"
                    android:layout_alignTop="@+id/summit_partners_para2"
                    android:layout_toLeftOf="@+id/summit_partners_para2"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="50dp"/>


                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="Deque Systems Inc"
                    android:layout_below="@+id/pic2"
                    android:layout_marginTop="-10dp"
                    android:textColor="#333"
                    android:textSize="16sp"/>
                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:text="@string/summit_partners_para3"
                    android:layout_below="@id/summit_partners_para2"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_marginTop="15dp"
                    android:textColor="#000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/summit_partners_para3"
                    android:autoLink="web"
                    android:textColorLink="#FF164165"/>



                <ImageView
                    android:id="@+id/pic3"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/centre_for_internet"
                    android:layout_alignTop="@+id/summit_partners_para3"
                    android:layout_toLeftOf="@+id/summit_partners_para3"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="50dp"/>

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="Centre for Internet and Society (CIS)"
                    android:layout_below="@+id/pic3"
                    android:layout_marginTop="3dp"
                    android:textColor="#333"
                    android:textSize="16sp"/>
                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:text="@string/summit_partners_para4"
                    android:layout_below="@id/summit_partners_para3"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_marginTop="15dp"
                    android:textColor="#000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/summit_partners_para4"
                    android:autoLink="web"
                    android:textColorLink="#FF164165"/>


                <ImageView
                    android:id="@+id/pic4"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/enable_india"
                    android:layout_alignTop="@+id/summit_partners_para4"
                    android:layout_toLeftOf="@+id/summit_partners_para4"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="50dp"/>

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="EnAble India"
                    android:layout_below="@+id/pic4"
                    android:layout_marginTop="3dp"
                    android:textColor="#333"
                    android:textSize="16sp"/>
                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:text="@string/summit_partners_para5"
                    android:layout_below="@id/summit_partners_para4"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_marginTop="15dp"
                    android:textColor="#000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/summit_partners_para5"
                    android:autoLink="web"
                    android:textColorLink="#FF164165"/>


                <ImageView
                    android:id="@+id/pic5"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/ability_foundation"
                    android:layout_alignTop="@+id/summit_partners_para5"
                    android:layout_toLeftOf="@+id/summit_partners_para5"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="50dp"/>


                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="Ability Foundation"
                    android:layout_below="@+id/pic5"
                    android:layout_marginTop="3dp"
                    android:textColor="#333"
                    android:textSize="16sp"/>
                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:text="@string/summit_partners_para6"
                    android:layout_below="@id/summit_partners_para5"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_marginTop="15dp"
                    android:textColor="#000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/summit_partners_para6"
                    android:autoLink="web"
                    android:textColorLink="#FF164165"/>


                <ImageView
                    android:id="@+id/pic6"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/vidhi"
                    android:layout_alignTop="@+id/summit_partners_para6"
                    android:layout_toLeftOf="@+id/summit_partners_para6"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="50dp"/>

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="Vidhi Centre for Legal Policy"
                    android:layout_below="@+id/pic6"
                    android:layout_marginTop="3dp"
                    android:textColor="#333"
                    android:textSize="16sp"/>
                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:text="@string/summit_partners_para7"
                    android:layout_below="@id/summit_partners_para6"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_marginTop="15dp"
                    android:textColor="#000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/summit_partners_para7"
                    android:autoLink="web"
                    android:textColorLink="#FF164165"/>


                <ImageView
                    android:id="@+id/pic7"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:src="@drawable/vidya_sagar"
                    android:layout_alignTop="@+id/summit_partners_para7"
                    android:layout_toLeftOf="@+id/summit_partners_para7"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="50dp"/>

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="Vidya Sagar"
                    android:layout_below="@+id/pic7"
                    android:layout_marginTop="3dp"
                    android:textColor="#333"
                    android:textSize="16sp"/>






            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
